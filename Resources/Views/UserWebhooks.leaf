<html>
<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
</head>
<script>
window.onload = function() {
  loadWebhooks()
};
function loadWebhooks() {

    fetch('/user/webhooks', {
    headers: {
    'Content-Type': 'application/json'
    }
    })
    .then((response) => {
      return response.json();
    })
    .then((triggers) => {
      console.log(triggers)
      for (i = 0; i < triggers.length; i++) {
        var trigger = triggers[i]
        var table = document.getElementById("triggertable");
    
        var row = document.createElement("TR");
        // a name for looking up the thing we will delete from the table later
        row.id = "trigger-"+trigger.id 
    
        table.appendChild(row);
        row.insertCell(0).innerHTML = trigger.swaggerRepo
        row.insertCell(1).innerHTML = trigger.gitRepo
        
        var checkbox = document.createElement("INPUT");
        checkbox.setAttribute("type", "checkbox");
        checkbox.id = trigger.id
        checkbox.name = "deletionCheckbox"
        var td = document.createElement('td');
        td.style.width = '80px';
        td.style.alignContent = "center";
        td.appendChild(checkbox)
        row.appendChild(td);    
      }
    });
    }

    </script>
<body onload="loadWebhooks()">
    <div class="container-fluid">
        
        <table class="table">
            <thead>
                <tr>
                    <th>id</th>
                    <th>verified</th>
                    <th>url</th>
                    <th>event</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td scope="row">a</td>
                    <td>a</td>
                    <td>b</td>
                    <td>c</td>
                </tr>
                <tr>
                    <td scope="row">a</td>
                    <td>e</td>
                    <td>d</td>
                    <td>c</td>
                </tr>
            </tbody>
        </table>
    </div>

